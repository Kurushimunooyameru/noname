#include <stdio.h>
int main(void)
{
int x;
printf("x = ") ;
scanf ("%i", &x);
if ((x%2) == 0) 
printf("четное ");
else 
printf("нечетное ");
if (x>99) 
printf("трехзначное ");
else 
if ((99>=x) & (x > 9)) 
printf("двухзначное ");
else 
if (x <= 9)  
printf("однозначное ");
printf("число.");
return 0;
}
