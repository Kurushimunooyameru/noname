#include <stdio.h>
int main(void)
{
    int a[20];
    int n;
    printf("N = ");
    scanf("%i", &n);
    int i;
    for (i = 0; i < n; ++i) {
        printf("a[%i] = ", i + 1);
        scanf("%i", &a[i]);
    }
    int i1;
    for (i = n - 1; i >= 0; --i) {
        if (a[i] > 0) {
            ++n;
            for (i1 = n - 1; i1 > i; --i1) a[i1] = a[i1 - 1];
            a[i] = 0;
        }
    }
    for (i = 0; i < n; ++i) printf("  %i = %i\n", i + 1, a[i]);
    return 0;
}
