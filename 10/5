#include <stdio.h>
#include <math.h>
main()
{
int k;
printf("k = ");
scanf("%d", &k);
k=abs (k);
if (k%10==k/1000 & k%100/10 == k%1000/100)
{
printf("Данное число читается одинаково слева направо и справа налево");
}
else
{
printf("Данное число не читается одинаково слева направо и справа налево");
}
return 0;
}
